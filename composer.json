{
    "name": "twineis/raven-php",
    "description": "Sentry (Raven) error monitoring for Laravel and Lumen with send in background via queues",
    "keywords": ["laravel", "sentry", "getsentry", "raven", "monitoring", "error", "logging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pulkit Jalan",
            "homepage": "https://github.com/pulkitjalan"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0",
        "illuminate/queue": "^6.0",
        "monolog/monolog": "~1.11 <=1.22",
        "sentry/sentry": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "~0.9.1"
    },
    "autoload": {
        "psr-4": {
            "Twine\\Raven\\": "src/"
        }
    }
}
